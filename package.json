{
    "name": "kb",
    "displayName": "kb",
    "description": "Language support for kb",
    "version": "0.0.2",
    "publisher": "mtots",
    "repository": {
        "type": "git",
        "url": "https://github.com/math4tots/kb_vscode"
    },
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "kb",
            "aliases": ["kb", "kb"],
            "extensions": [".kb"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "kb",
            "scopeName": "source.kb",
            "path": "./syntaxes/kb.tmLanguage.json"
        }]
    }
}